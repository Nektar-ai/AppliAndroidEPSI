<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.epsi.tp_poisson_personne">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TP_Poisson_Personne">
        <activity android:name=".ProductActivity"
            android:parentActivityName=".ProductsActivity" />
        <activity android:name=".ProductsActivity"
            android:parentActivityName=".CategoriesActivity" />
        <activity android:name=".CategoriesActivity"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".StudentActivity"
            android:parentActivityName=".GroupActivity" />
        <activity android:name=".GroupActivity"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>